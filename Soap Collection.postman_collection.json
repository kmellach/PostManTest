	"item": [
		{
			"name": "Celsius to Farenheit",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"",
							"const jsonData = xml2Json(pm.response.text())",
							"",
							"const degrees = (jsonData[\"soap:Envelope\"][\"soap:Body\"][\"CelsiusToFahrenheitResponse\"][\"CelsiusToFahrenheitResult\"])",
							"",
							"function celsiusToFahrenheit(celsius) {",
							"    return (celsius * 9/5) + 32;",
							"}",
							"",
							"pm.test('Validate response code is 200', function(){",
							"    pm.response.to.have.status(200)",
							"})",
							"",
							"pm.test('Validate response contain valid fahrenheitValue', function(){",
							"    const celsius = pm.collectionVariables.get(\"celsius\");",
							"    pm.expect(degrees).to.be.a('string')",
							"    pm.expect(parseInt(degrees)).to.eq(celsiusToFahrenheit(parseInt(celsius)))",
							"})"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const celsius = pm.collectionVariables.get(\"celsius\");"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
